    "build": "rm -rf ./dist && tsc",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/typescript": "^2.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mysql2": "^3.11.3",
    "nodemon": "^3.1.7",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "0.3.7",
    "typescript": "^5.6.3"
  }
}
